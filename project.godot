boot_splash/bg_color=Color(0.141176, 0.141176, 0.141176, 1)
config/icon="res://Sprites/icon.svg"

[autoload]

ScrGameManager="*res://Scripts/GameScripts/scr_game_manager.gd"
ScrPlayerGeneral="*res://Scripts/PlayerScripts/PlayerGeneralScripts/scr_player_general.gd"
