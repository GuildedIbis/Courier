window/size/viewport_height=270
window/size/window_width_override=1440
window/size/window_height_override=810
window/stretch/mode="canvas_items"

[dotnet]

project/assembly_name="CourierBeta1"

[file_customization]

folder_colors={
"res://Scenes/": "purple",
"res://Scripts/": "yellow",
"res://Sprites/": "red"
